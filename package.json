{
  "name": "keystroke-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "server": "node server.js",
    "server:dev": "nodemon server.js --watch server.js --ext js",
    "ui": "cd keystroke-ui && npm run dev",
    "dev": "concurrently -k \"npm:server:dev\" \"wait-on http://localhost:5000/health && npm run ui\"",
    "test:post": "node tools/test_post.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "encodings": "^1.0.0",
    "express": "^4.22.1",
    "iconv-lite": "^0.7.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.11",
    "start-server-and-test": "^2.1.3",
    "wait-on": "^9.0.3"
  }
}
